import json
import sys
import os

ghComp = ghenv.Component
ghDoc = ghComp.OnPingDocument()

# the full path of the gh file
HERE = ghDoc.FilePath
print('HERE:\n{}'.format(HERE))
# the current directory 
filename += ".json"
path = os.path.abspath(os.path.join(HERE, "..", "data", filename))
print('filepath:\n{}'.format(path))

if load:
    
    # JSON file to read
    with open(path, mode='r') as file:
    
        # Load the data
        data = json.load(file)
        
        # access a sample key
        zero = data['zero']
        print(zero)
        print('{}.json has been successfully loaded!'.format(filename))
